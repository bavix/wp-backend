{
    "name": "bavix/wheelpro",
    "description": "Easy wheels search.",
    "keywords": [
        "wheelpro",
        "wheels"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:bavix/laravel-nova.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rez1dent3/chat.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rez1dent3/laravel-acl.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rez1dent3/laravel-addresses.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rez1dent3/laravel-support.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rez1dent3/laravel-cacheable.git"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaFileField"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaBrandFilter"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "bavix/laravel-cupkit": "^0.1",
        "bavix/laravel-middleware-ifindex": "^1.0",
        "bavix/nova-brand-filter": "*",
        "bavix/nova-file-field": "*",
        "bolechen/nova-activitylog": "^0.1",
        "coderello/laravel-nova-lang": "^1.1",
        "davidpiesse/nova-map": "^0.0",
        "doctrine/dbal": "^2.9",
        "embed/embed": "^3.4",
        "fideloper/proxy": "^4.2",
        "gregoriohc/laravel-nova-theme-responsive": "^0.6",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/nova": "^2.1",
        "laravel/passport": "^7.4",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "musonza/chat": "^3.4",
        "orlyapps/nova-belongsto-depend": "^2.0",
        "pda/pheanstalk": "^4.0",
        "predis/predis": "^1.1",
        "r64/nova-image-cropper": "^1.1",
        "rcknr/nova-multiselect-filter": "^1.2",
        "rennokki/befriended": "^1.2",
        "rinvex/laravel-addresses": "^2.1",
        "spatie/eloquent-sortable": "^3.6",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-query-builder": "^2.1",
        "spatie/nova-tags-field": "^2.2",
        "wapmorgan/morphos": "^3.2",
        "yajra/laravel-acl": "^4.0",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "filp/whoops": "^2.5",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan nova:publish",
            "@php artisan nova-lang:publish ru --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
