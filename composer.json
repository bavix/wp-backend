{
    "name": "bavix/wheelpro",
    "description": "Easy disk search.",
    "keywords": ["wheelpro", "wheels"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:bavix/laravel-nova.git"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "barryvdh/laravel-cors": "^0.11",
        "bavix/laravel-admin-leaflet": "^1.0",
        "bavix/laravel-cupkit": "^0.1",
        "bavix/laravel-middleware-ifindex": "^1.0",
        "coderello/laravel-nova-lang": "^1.0",
        "davidpiesse/nova-map": "^0.0.5",
        "doctrine/dbal": "^2.9",
        "embed/embed": "^3.3",
        "encore/laravel-admin": "^1.6",
        "fideloper/proxy": "^4.1",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel-admin-ext/config": "^1.0",
        "laravel-admin-ext/helpers": "^1.1",
        "laravel/framework": "5.8.*",
        "laravel/nova": "^2.0",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "musonza/chat": "^3.3",
        "orlyapps/nova-belongsto-depend": "^2.0",
        "pda/pheanstalk": "^4.0",
        "predis/predis": "^1.1",
        "rennokki/befriended": "^1.3",
        "rinvex/laravel-addresses": "^2.0",
        "rinvex/laravel-attributes": "^2.0",
        "spatie/eloquent-sortable": "^3.5",
        "spatie/laravel-query-builder": "^1.17",
        "wapmorgan/morphos": "^3.2",
        "yajra/laravel-acl": "^3.6",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan nova:publish",
            "@php artisan nova-lang:publish ru --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
