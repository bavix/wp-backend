{
    "name": "bavix/nova-brand-filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bavix\\NovaBrandFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bavix\\NovaBrandFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
